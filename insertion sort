function insertionsort(arr) {
  for (let i = 1; i < arr.length; i++) {
    let currentvalue = arr[i];
    for (var j = i - 1; j >= 0 && arr[j] > currentvalue; j--) {
      arr[j + 1] = arr[j];
    }
    arr[j + 1] = currentvalue;
  }
  return arr;
}
console.log(insertionsort([22, 5, 6, 14, 9, 15]));
